
body {
  font-family: Arial, sans-serif;
  text-align: center;
  background: #f9f9f9;
  margin: 0;
  padding: 20px;
}

h1 {
  color: #333;
}

.gallery {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  gap: 15px;
  margin-top: 30px;
}

.gallery img {
  width: 200px;
  height: auto;
  border: 2px solid #ccc;
  border-radius: 8px;
}
